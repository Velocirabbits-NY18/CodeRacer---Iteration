  "description": "Practice typing code alone or with other users to improve typing speed",
  "main": "index.js",
  "scripts": {
    "start": "set \"NODE_ENV=production\" && nodemon server/server.js",
    "build": "set \"NODE_ENV=production\" && webpack",
